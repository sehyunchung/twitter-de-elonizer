{
  "manifest_version": 3,
  "name": "Twitter De-Elonizer",
  "version": "0.1.0",
  "description": "A simple Chrome extension to remove some of the disasters from a disaster.",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["*://*.twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
